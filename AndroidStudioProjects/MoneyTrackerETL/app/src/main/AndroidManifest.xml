        </activity>
        <activity android:name=".Home" />
        <activity android:name=".firstpage" />
        <activity
            android:name=".navigationactivity"
            android:label="Home"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Deposit"
